[[customizations.user]]
name = "{{ bootc_image_user_name }}"
password = "{{ _bootc_image_password_hash }}"
groups = {{ bootc_image_user_groups | to_json }}
{% if _bootc_image_ssh_keys | default([]) | length > 0 %}
key = """
{% for key in _bootc_image_ssh_keys %}
{{ key }}
{% endfor %}
"""
{% endif %}
